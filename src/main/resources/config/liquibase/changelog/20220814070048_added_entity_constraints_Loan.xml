<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Loan.
    -->
    <changeSet id="20220814070048-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="loan_attribute_id"
                                 baseTableName="loan"
                                 constraintName="fk_loan__loan_attribute_id"
                                 referencedColumnNames="id"
                                 referencedTableName="loan_attribute"/>

        <addForeignKeyConstraint baseColumnNames="applicant_id"
                                 baseTableName="loan"
                                 constraintName="fk_loan__applicant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="applicant"/>

        <addForeignKeyConstraint baseColumnNames="loan_requirement_id"
                                 baseTableName="loan"
                                 constraintName="fk_loan__loan_requirement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="loan_requirement"/>
    </changeSet>
</databaseChangeLog>
